[configuration]

entry_symbol = "godot_apple_plugins_start"
compatibility_minimum = 4.4

[libraries]
ios.debug.arm64 = "./bin/debug/GodotApplePlugins.xcframework"
macos.debug.arm64 = "./bin/debug/GodotApplePlugins.framework"
